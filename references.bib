  doi = {10.1143/PTPS.146.169},
  abstract = {Light N=Z nuclei show pronounced clustering for exited states close to the decay thresholds. This fact is expressed in the Ikeda-Diagram. In neutron-rich nuclei, low lying states close to the neutron drip-line show also pronounced clustering with {$\alpha$}-particles and heavier closed shell clusters. For the valence neutrons a concept based on molecular orbitals can be used for their single particle wave functions. Covalent binding between {$\alpha$}-particles due to valence neutrons and a related shallow local potential is now known to be responsible for particular structures in light neutron rich nuclei, like dimers in Be-isotopes, and trimers in C-isotopes. A local potential as between two {$\alpha$}-particles can be defined between other clusters, for example between {$\alpha$}-particles and 16O, giving rise to intrinsic reflection asymmetric molecular structures in 21Ne as parity doublets. An Extended Ikeda Diagram can be constructed based on these concepts with clusters and covalent neutrons.},
  keywords = {nuclear molecules},
  file = {/home/angus/Zotero/storage/X2V6LXDT/von Oertzen - 2002 - Nuclear Clusters and Covalently Bound Nuclear Mole.pdf;/home/angus/Zotero/storage/WPPAVHHA/1860641.html}
}

@article{von_oertzen_nuclear_2006,
