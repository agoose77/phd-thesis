  shorttitle = {Studying Physics during the {{COVID-19}} Pandemic},
  author = {Klein, P. and Ivanjek, L. and Dahlkemper, M. N. and Jeli{\v c}i{\'c}, K. and Geyer, M.-A. and K{\"u}chemann, S. and Susac, A.},
