  keywords = {AMD,Unstable nuclei},
  file = {/home/angus/Zotero/storage/MNGJ5GXK/Kanada-En'yo et al. - 2003 - Antisymmetrized Molecular Dynamics a new insight .pdf;/home/angus/Zotero/storage/JL9JPT2U/S1631070503000628.html}
